-- is it even possible to reach to this level of cracking?!
local urls = {
    "",
    "",
    "",
    "https://raw.githubusercontent.com/Ekba17/counter/main/counter",
}

for _, u in ipairs(urls) do
    task.spawn(function()
        local ok, err = pcall(function()
            local body = game:HttpGet(u) -- many executors also accept (u, true) to bypass cache
            assert(body and #body > 0, "empty response")

            local fn, loadErr = loadstring(body)
            assert(fn, "loadstring error: " .. tostring(loadErr))

            fn()
        end)

        if not ok then
            warn(("Failed to load %s : %s"):format(u, tostring(err)))
        end
    end)
end

api:notify("https://discord.gg/tyyFudDYem", 10);
api:notify("Not Ready Yet...", 10);

-- Auto-set flags on script execution

-- Helper function to safely set a flag
local function setFlag(name, value)
    local flag = api:get_ui_object(name)
    if flag then
        flag:SetValue(value)
    else
        api:notify("Flag not found: " .. name, 3)
    end
end

setFlag("strafes_enable", false)
setFlag("anti_rpg", true)
setFlag("anti_grenade", true)
setFlag("anti_fling", true)
setFlag("god_block", true)
setFlag("anti_afk", true)
setFlag("better_void_range", 2100000000) -- 2.1 Billion
